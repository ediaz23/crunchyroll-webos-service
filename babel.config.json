{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "0.12.2"
                },
                "useBuiltIns": "usage",
                "corejs": 3,
                "exclude": [
                    "transform-regenerator"
                ]
            }
        ]
    ],
    "plugins": [
        "babel-plugin-transform-async-to-promises"
    ]
}